	function setComma(str) {
		str = String(str);

		return str.replace(/(\d)(?=(?:\d{3})+(?!\d))/g, '$1,');
	}


	function setFlor(num) {
		var strNum = num;
		if (strNum < 0 ) {
			return 'B'+ strNum;
		} else if (strNum > 0 ) {
			return strNum + 'F';
		} else {
			return '-';
		}
	}

	function setIsNaNCheck(strVal) {
		if (isNaN(strVal)) { strVal = 0; }
		return strVal;
	}

